<h1>Images</h1>

<% if @create_ownerships.any? %>
	<%= link_to "Ajouter", new_admin_image_path, class: "w8-button l-blue" %>
<% end %>

<div id="admin_image">
	<% @elements.each do |image| %>
		<%= image_tag image.image_url %>
		<%= link_to "<i class='foundicong-remove'></i>".html_safe, admin_image_path(image), method: :delete, confirm: "Etes-vous sÃ»r de vouloir supprimer cette image ?" %>
		<a class="url_link" data-url="<%= image.image_url %>" href="#"><i class='foundicong-location'></i></a>
	<% end %>
</div>